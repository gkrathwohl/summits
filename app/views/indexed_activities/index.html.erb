<p id="notice"><%= notice %></p>

<h1>Listing Indexed Activities</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Activityid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @indexed_activities.each do |indexed_activity| %>
      <tr>
        <td><%= indexed_activity.userId %></td>
        <td><%= indexed_activity.activityId %></td>
        <td><%= link_to 'Show', indexed_activity %></td>
        <td><%= link_to 'Edit', edit_indexed_activity_path(indexed_activity) %></td>
        <td><%= link_to 'Destroy', indexed_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indexed activity', new_indexed_activity_path %>
